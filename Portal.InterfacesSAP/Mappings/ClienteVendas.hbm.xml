<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Portal.DadosSap"
                   namespace="Portal.DadosSap.Entity" >

  <class name="ClienteVendas" table="pro_cliente_vendas">

    <composite-id>
      <key-property name="Id_cliente"/>
      <key-property name="Org_vendas"/>
      <key-property name="Can_dist"/>
      <key-property name="Set_ativ"/>
    </composite-id>
   
    <property name="Grupo_cli" not-null="false" access="property" type="String">
      <column name="Grupo_cli" length="3" />
    </property>

    <property name="Id_fornecedor" not-null="false" access="property" type="String">
      <column name="Id_fornecedor" length="18" />
    </property>
    
    <property name="Data_criacao" not-null="false" access="property" type="DateTime">
      <column name="Data_criacao"/>
    </property>

    <property name="Pacote" not-null="false" access="property" type="String">
      <column name="Pacote" length="30" />
    </property>

    <property name="Hora_criacao" not-null="false" access="property" type="String">
      <column name="Hora_criacao" length="8" />
    </property>

    <property name="Denominacao" not-null="false" access="property" type="String">
      <column name="Denominacao" length="40" />
    </property>

    <property name="Eliminacao" not-null="false" access="property" type="String">
      <column name="Eliminacao" length="1" />
    </property>

  </class>

</hibernate-mapping>
