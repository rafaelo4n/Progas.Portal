    <property name="Grupo_cli" not-null="false" access="property" type="String">
      <column name="Grupo_cli" length="3" />
    </property>

    <property name="Id_fornecedor" not-null="false" access="property" type="String">
      <column name="Id_fornecedor" length="18" />
    </property>
    
    <property name="Data_criacao" not-null="false" access="property" type="DateTime">
      <column name="Data_criacao"/>
