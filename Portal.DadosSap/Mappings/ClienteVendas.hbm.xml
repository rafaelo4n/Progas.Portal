<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Portal.DadosSap"
                   namespace="Portal.DadosSap.Entity" >

  <class name="ClienteVendas" table="pro_cliente_vendas">

